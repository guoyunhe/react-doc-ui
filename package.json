    "library",
    "demo",
    "playground",
    "syntax-highlight"
  ],
  "homepage": "https://guoyunhe.github.io/react-doc-ui/",
  "bugs": {
    "url": "https://github.com/guoyunhe/react-doc-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guoyunhe/react-doc-ui.git"
